.logo-area
	position absolute
	z-index 3
	top 0px
	left 0px
	width 100%

	.logo
		background url(images/GoE_Logo.png) no-repeat top left
		margin 100px auto 0px;
		width 808px
		height 637px
		background-size 100% 100%

		@media(max-width 800px)
			width 600px
			height 450px

		@media(max-width 600px)
			width 320px
			height 240px
			margin-top 90px
			
	.coming-soon
		background url(images/Coming-Soon.png) no-repeat top left
		margin-left auto
		margin-right auto
		margin-top 0px
		height 85px
		width 500px
		background-size 100% 100%

		@media(max-width 600px)
			width 260px
			height 40px